error codes {
4 int 
first int {
    dependency error 0 {
        second int {
            ffmpeg 1;
            surrealdb 2;
            nix 3;
            net tools 4;
            rocks db 5;
            clang 6;
            
        }
    }
    os error 1 {
        normal linux distro 0,
        mac uncompatiple 1,
        windows uncompatiple 2
        uncompatiple os 3,
    }
}

third int {
    type of error 
    
    not found 0;,
    old version 1;,
    unresponseve 2;,
    uncompatiple 3;,
    
}
fourth int {
    exited with the error 1,
    exeted normally 0,
    }

example {
    ffmped not found: 0101;
    os uncompatiple: 1331;
    nix not found: 0300;

}
}





